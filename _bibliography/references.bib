@article{yueDevelopmentStop2002,
  title = {Development of the {{Stop Endings}} in the {{Yue Dialects}}},
  author = {Yue, Anne},
  date = {2002-01-01},
  abstract = {It has been claimed that the Yue dialects best preserve the stop endings of Middle Chinese *-p, *-t and *-k. The bulk of dialect materials available at present contain not only Yue dialects that faithfully or almost faithfully preserve these endings, but also those that have displayed change to various degrees. This array of variety enables us to better understand the nature and conditions for change with respect to these endings and to figure out some general tendency in the principles guiding the change. In tracing the development of the stop endings of various dialects from proto-Yue, several general principles of change have been observed: (1) assimilation is the key word for the condition of the change in the great majority of cases. The change from *-p or *-k to -t is largely due to assimilation of the ending to a higher vowel while the change from *-t or from *-p to -k is due to assimilation of the ending to a lower vowel; (2) syllables derived from proto-Yue with tense vowels are more prone to change; (3) the complete loss of ending consonants are closely related to two factors, tenseness of the vowel and higher tonal value; (4) the rate of change differs from dialect to dialect and within the same dialect, from lexical item to lexical item.},
  keywords = {Chinese,dialectology,historical phonology,yue}
}

@article{zhangEarlyMixed2012,
  title = {Early {{Mixed Farming}} of {{Millet}} and {{Rice}} 7800 {{Years Ago}} in the {{Middle Yellow River Region}}, {{China}}},
  author = {Zhang, Jianping and Lu, Houyuan and Gu, Wanfa and Wu, Naiqin and Zhou, Kunshu and Hu, Yayi and Xin, Yingjun and Wang, Can},
  date = {2012-12-17},
  journaltitle = {PLoS ONE},
  shortjournal = {PLoS One},
  volume = {7},
  number = {12},
  eprint = {23284907},
  eprinttype = {pmid},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0052146},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3524165/},
  urldate = {2021-03-09},
  abstract = {The Peiligang Culture (9000-7000 cal. yr BP) in the Middle Yellow River region, North China, has long been considered representative of millet farming. It is still unclear, however, if broomcorn millet or foxtail millet was the first species domesticated during the Peiligang Culture. Furthermore, it is also unknown whether millet was cultivated singly or together with rice at the same period. In this study, phytolith analysis of samples from the Tanghu archaeological site reveals early crop information in the Middle Yellow River region, China. Our results show that broomcorn millet was the early dry farming species in the Peiligang Culture at 7800 cal. yr BP, while rice cultivation took place from 7800 to 4500 cal. yr BP. Our data provide new evidence of broomcorn millet and rice mixed farming at 7800 cal. yr BP in the Middle Yellow River region, which has implications for understanding the domestication process of the two crops, and the formation and continuance of the Ancient Yellow River Civilization.},
  pmcid = {PMC3524165},
  keywords = {rice}
}

@report{zhangReconstructionLost2019,
  title = {A {{Reconstruction}} of the {{Lost Chronology}} of {{Zhou}}},
  author = {Zhang, Pengcheng},
  date = {2019-11-27T09:57:31},
  institution = {{SocArXiv}},
  doi = {10.31235/osf.io/vfc23},
  url = {https://osf.io/preprints/socarxiv/vfc23/},
  urldate = {2021-06-09},
  abstract = {The first half of the Zhou Dynasty, known as Western Zhou, was a critical formative period of Chinese civilization.  Western Zhou introduced philosophical ideas, cultural motifs, governing practices, and social institutions that have had a profound and lasting impact on Chinese and East Asian societies.  Unfortunately, only a partial chronology of this time period survives.  Recovering the absolute dates of Western Zhou has since become a classical question that remained unresolved for over two millennia.  Here I report a successful reconstruction of this long-sought chronology using archaeoastronomical methods.  Systematic analysis of the relative compatibility of all 81 known fully-dated bronze inscriptions revealed the definitions of the lunar phase terms, which enabled computation of absolute dates.  These calculations, combined with available archaeological and textual evidence, produced a complete chronology starting in the year King Wen received the Mandate of Heaven and ending in the year of King Ping's death.  The results also reveal a previously unknown seven-year gap in the regnal years of Zhou, which explains the inconsistencies between excavated material and the annals of Shiji.  The complete chronology reported here is highly consistent with archaeological evidence, textual records, and astronomical events, thus providing a reliable temporal framework for studying ancient Chinese history, and facilitating a deeper understanding of the origins of Chinese civilization.},
  keywords = {Ancient China,Ancient History,Archaeoastronomy,Arts and Humanities,Asian History,Bronze Inscriptions,Bronze Vessels,China,Chinese,Chinese History,Chinese Studies,Chronology,Early China,East Asian Languages and Societies,history,History,Lunar Phase,Lunation,Western Zhou,zhou,Zhou Dynasty,先秦史,初吉,历史,周王纪年,年代学,既望,既死霸,既生霸,月相术语,月相辞,竹书纪年,系年,西周年代,西周纪年,金文,金文历日,金文历谱,青铜器}
}

