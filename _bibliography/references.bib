@article{normanPharyngealizationEarly1994,
  title = {Pharyngealization in {{Early Chinese}}},
  author = {Norman, Jerry},
  date = {1994},
  journaltitle = {Journal of the American Oriental Society},
  volume = {114},
  number = {3},
  eprint = {605083},
  eprinttype = {jstor},
  pages = {397--408},
  publisher = {{American Oriental Society}},
  issn = {0003-0279},
  doi = {10.2307/605083},
  abstract = {A prominent feature of Middle Chinese, a linguistic system reconstructed on the basis of the Chiehyunn dictionary composed by Luh Faayan, is palatalization. In most reconstructions of Middle Chinese, this palatalization is manifested by the presence of a palatal glide after the initial consonant. More than half of all Middle Chinese syllables belong to the palatalized category. The present paper is an attempt to provide an explanation for this typologically rather unusual state of affairs. Early transcriptional evidence, Sino-Tibetan comparison, and certain Chinese dialect forms all suggest that the Middle Chinese palatalization was of secondary origin. Using the notion of phonological markedness, the present paper proposes that palatalization came about in Middle Chinese unless impeded by some other feature. In this model of Early Chinese, syllables were of three basic types: pharyngealized and retroflexed syllables prevented palatalization while plain syllables gave rise to palatalized syllables in Middle Chinese. The result of this analysis is a simple syllabic structure in Early Chinese in which there are no post-initial glides at all.}
}

